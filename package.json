{
  "name": "docterbee-media-ai",
  "version": "1.0.0",
  "description": "Docterbee Media & AI Server with Gemini API",
  "type": "module",
  "main": "backend/server.mjs",
  "scripts": {
    "start": "node backend/server.mjs",
    "dev": "node --watch backend/server.mjs",
    "lint": "eslint backend/**/*.mjs js/**/*.js --ignore-pattern node_modules",
    "lint:fix": "eslint backend/**/*.mjs js/**/*.js --fix --ignore-pattern node_modules",
    "test": "node --test tests/",
    "migrate:seed-rewards": "node -e \"import('./backend/migrations/seed-default-rewards.mjs').then(m => m.up())\"",
    "create-admin": "node backend/scripts/create-default-admin.mjs",
    "setup": "npm run create-admin && npm run migrate:seed-rewards"
  },
  "keywords": [
    "docterbee",
    "gemini",
    "ai",
    "health",
    "islamic"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-session": "^1.18.2",
    "express-validator": "^7.3.1",
    "multer": "^2.0.2",
    "mysql2": "^3.11.5",
    "resend": "^6.6.0",
    "youtube-transcript": "^1.2.1",
    "youtubei.js": "^16.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "isomorphic-dompurify": "^2.35.0"
  }
}
